<?xml version="1.0"?>
<digester-rules>
	<pattern value="root">
		<object-create-rule classname="com.hisun.atc.fil.HiRoot" />
		<set-properties-rule />
		<call-method-rule methodname="loadAfter" />
	</pattern>
	<pattern value="*/In">
		<object-create-rule classname="com.hisun.atc.fil.HiInDesc" />
		<set-properties-rule />
		<set-next-rule methodname="addChilds"/>
		<set-top-rule methodname="setParent"/>
		<call-method-rule methodname="loadAfter"/>
	</pattern>
	<pattern value="*/Out">
		<object-create-rule classname="com.hisun.atc.fil.HiOutDesc" />
		<set-properties-rule />
		<set-next-rule methodname="addChilds"/>
		<set-top-rule methodname="setParent"/>
		<call-method-rule methodname="loadAfter"/>
	</pattern>
	<include path="config/ITF_RULES.XML" />

</digester-rules>

<!DOCTYPE databases PUBLIC
"-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
"http://castor.exolab.org/mapping.dtd">
<mapping>
    <description>协议数据项配置的映射文件</description>
    <class name="pep.bp.realinterface.conf.ProtocolCommandItems" auto-complete="true">
        <map-to xml="ProtocolCommandItems"/>
        <field name="CommandItems" collection="map" type="pep.bp.realinterface.conf.ProtocolCommandItem">
            <bind-xml name="CommandItem"/>
        </field>
    </class>
    <class name="pep.bp.realinterface.conf.ProtocolCommandItem" auto-complete="true">
        <map-to xml="CommandItem"/>
        <field name="CommandCode">
            <bind-xml name="CommandCode" node="attribute"/>
        </field>
        <field name="DataItems" collection="map" type="pep.bp.realinterface.conf.ProtocolDataItem">
            <bind-xml name="DataItem"/>
        </field>
    </class>
    <class name="pep.bp.realinterface.conf.ProtocolDataItem" auto-complete="true">
        <field name="Length">
            <bind-xml name="Length" node="attribute"/>
        </field>
        <field name="Format">
            <bind-xml name="Format" node="attribute"/>
        </field>
        <field name="DataItemCode">
            <bind-xml name="DataItemCode" node="attribute"/>
        </field>
        <field name="GroupCode">
            <bind-xml name="GroupCode" node="attribute"/>
        </field>
    </class>
</mapping>